{
  "name": "backend",
  "version": "0.7.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "rm -rf dist && nest start --watch",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/config/typeorm-cli.config.ts",
    "typeorm:prod": "npx typeorm migration:run -f src/config/typeorm-cli.config"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.13",
    "@nestjs/config": "^0.4.2",
    "@nestjs/core": "^7.0.13",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.13",
    "@nestjs/platform-socket.io": "^7.0.13",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^7.0.13",
    "algorithmia": "^0.3.10",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.1.0",
    "googleapis": "^51.0.0",
    "jsonwebtoken": "^8.5.1",
    "nestjs-stripe": "^0.4.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "sbd": "^1.0.18",
    "stripe": "^8.53.0",
    "typeorm": "^0.2.25",
    "watson-developer-cloud": "^4.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.13",
    "@types/express": "^4.17.6",
    "@types/jest": "25.2.3",
    "@types/node": "^14.0.3",
    "@types/socket.io": "^2.1.6",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
